		<formula>
			<forall var = "v1" in = "pat_dual_mixed_1">
				<forall var = "v2" in = "pat_dual_mixed_2">
					<implies>
						<and>
							<not>
								<bfunction name = "same">
									<param pos = "1" var = "v1" field = "subject" />
									<param pos = "2" var = "v2" field = "subject" />
								</bfunction>
							</not>
							<bfunction name = "sz_loc_close">
								<param pos = "1" var = "v1" field = "object" />
								<param pos = "2" var = "v2" field = "object" />
							</bfunction>
